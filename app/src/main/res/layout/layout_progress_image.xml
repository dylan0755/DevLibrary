<?xml version="1.0" encoding="utf-8"?>
<com.dylan.mylibrary.ui.onlinepic.glide.progress.ProgressImageLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

    <!--背景透明-->
    <com.dylan.mylibrary.ui.onlinepic.DragPhotoView
        android:id="@+id/photoView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#0000"
       />

    <com.dylan.mylibrary.ui.onlinepic.glide.progress.CircleProgressView
        android:id="@+id/progressView"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_centerInParent="true"
        android:progress="1"
        android:visibility="gone"
        app:cpv_innerPadding="2dp"
        app:cpv_outerColor="#E5ffffff"
        app:cpv_outerSize="1dp"
        app:cpv_progressNormalColor="@color/transparent"
        app:cpv_progressReachColor="#E5ffffff"
        app:cpv_progressStyle="FillInnerArc" />

</com.dylan.mylibrary.ui.onlinepic.glide.progress.ProgressImageLayout>